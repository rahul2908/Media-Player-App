<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.mediaplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/music"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlayingActivity"
            android:label="Now Playing"/>
        <activity
            android:name=".ArtistsActivity"
            android:label="Artists"/>
        <activity
            android:name=".AlbumsActivity"
            android:label="Albums"/>
        <activity
            android:name=".SongsActivity"
            android:label="Songs"/>
        <activity
            android:name=".PlaylistsActivity"
            android:label="Playlists"></activity>
    </application>

</manifest>